    <py_script># python script
import requests
import json
import pandas as pd

""" Get AEMET endpoints """
if variables.date_from[0] != "":
	responseEndpoints = requests.get("https://opendata.aemet.es/opendata/api/valores/climatologicos/diarios/datos/fechaini/"+str(variables.date_from[0])+"T00:00:00UTC/fechafin/"+str(variables.date_to[0])+"T00:00:00UTC/todasestaciones?api_key="+ variables.api_key[0], stream=True)


endpoints = json.loads(responseEndpoints.text)
endpointResponseCode = endpoints['estado']

dataAEMET = ""
rawData=""

if endpointResponseCode == 200:
	dataAEMET = json.loads(requests.get(endpoints['datos'], stream=True).text)
else:
	rawData = endpoints
</py_script>
